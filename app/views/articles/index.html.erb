<header class="navbar border-bottom">
  <h4 class="ms-3 p-2"><%= link_to "Wikinimous", articles_path %></h4>
  <h5 class="me-3 p-2"><%= link_to "New Article", new_article_path %></h5>
</header>

<div class="container pt-2">
  <% @articles.each do |article| %>
  <div>
    <h3><%= link_to article.title, article_path(article.id) %></h3>
    <p><%= article.content %></p>
  </div>
  <% end %>

</div>
